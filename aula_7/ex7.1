	.data
prompt:	.asciiz "Introduza um numero.\n"
par:	.asciiz	"o numero é par!\n"
promptimpar:	.asciiz	"o numero é impar!\n"

	.text
	.globl main
	
main:	
	la $a0, prompt
	li $v0, 4
	syscall
	
	li $v0, 5
	syscall
	move $t0, $v0
	
	andi $t1, $t0, 1
	bne $t1, $0, impar
	
	la $a0, par
	li $v0, 4
	syscall
	j end
	
impar:	la $a0, promptimpar
	li $v0, 4
	syscall
end: